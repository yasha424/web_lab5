{
    "env": {
        "browser": true,
        "es2021": true
    },
    "plugins": ["sonarjs"],
    "extends": ["standard", "plugin:sonarjs/recommended"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "quotes": ["error", "single"],
        "semi": 0,
        "indent": ["error", 4]
    }
}
